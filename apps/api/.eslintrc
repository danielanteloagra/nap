{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "import/extensions": [1, {}],
    "import/no-extraneous-dependencies": [1, { "devDependencies": true }],
    "import/no-unresolved": [1, {}],
    "no-unused-expressions": [1, {}]
  }
}
